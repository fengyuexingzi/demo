.app {
  position: static;
  font-family: Helvetica Neue,
                 Tahoma,
                 Arial,
                 PingFangSC-Regular,
                 Hiragino Sans GB,
                 Microsoft Yahei,
                 sans-serif;
  text-align: center;
  color: #3c3c3c;
  user-select: none;
  height: 100%;
  font-size: 22.88rpx;
}

.app-shell {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}

.app-view-wrapper {
  position: relative;
  overflow: hidden;
  width: 100%;
  height: 100%;
  max-width: 748rpx;
  margin: 0 auto;
}

.container {
  margin-right: auto;
  margin-left: auto;
  flex-basis: 100%;
}

.app-view {
  position: absolute;
  z-index: 999;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  overflow-x: hidden;
  overflow-y: auto;
  transition: transform 0.4s;
  will-change: transform;
  background: #fff;
  color: #3c3c3c;
}

.container.fluid {
  max-width: 100%;
  width: 100%;
}

.app-view-with-footer {
  padding-bottom: 0;
}

.header {
  position: fixed;
  top: -1px;
  left: 0;
  right: 0;
  z-index: 99;
  box-shadow: 0 2rpx 4rpx -1rpx rgba(0, 0, 0, 0.2);
  background: #f2f2f2;
  line-height: 1.15;
}

.app-header-wrapper {
  display: flex; /*弹性盒子*/
  flex-direction: row; /*方向*/
  flex-wrap: nowrap; /*换行*/
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  background: #f2f2f2;
  color: #666;
  padding: 0;
}

.app-header-middle {
  flex: 1;
  display: flex;
  align-items: center;
  color: #666;
  font-size: 36rpx;
}

.app-header-item {
  width: 60rpx;
  margin: 0 20rpx;
}

.app-header-title {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border: 1px solid #e5e5e5;
  text-align: left;
  width: 100%;
  color: rgba(0, 0, 0, 0.3);
  background-color: #fff;
  border-radius: 5rpx;
}

.app-header-icon {
  display: block;
  width: 60rpx;
  height: 60rpx;
}

.image-icons {
  display: inline-block;
  background-color: transparent;
  background-repeat: no-repeat;
  background-position: 50% center;
}

.icon-logo {
  background-image: url(data:image/png;base64,);
  background-size: 45rpx 45rpx;
}

.icon-search {
  background-image: url(data:image/png;base64,);
  background-size: 45rpx 45rpx;
}

.icon-user {
  background-image: url(data:image/png;base64,);
  background-size: 45rpx 45rpx;
}

.nav {
  overflow-x: auto;
  background: #f2f2f2;
  font-size: 28rpx;
  white-space: nowrap;
  z-index: 2;
}

.nav-item {
  display: inline-block;
  padding: 0 26rpx;
}

.nav-item span {
  display: inline-block;
  line-height: 64rpx;
  border-bottom: 4rpx solid rgba(237, 91, 0, 0);
}

.page-wrap {
  position: relative;
  height: 100%;
  overflow-x: hidden;
  overflow-y: auto;
}

.bodies {
  position: absolute;
  top: 164rpx;
  left: 0;
  right: 0;
  background: #fff;
  transition: transform 0.4s;
}

.component-list-main {
    height: 100vh;
}


.swiper-container {
  position: relative;
  overflow: hidden;
  z-index: 1;
}

.swiper-container block {
  display: block;
}

swiper-item image {
  width: 100%;
}

.divider-line {
  height: 18rpx;
  border-bottom: 18rpx solid rgb(245, 245, 245);
  background-color: rgb(245, 245, 245);
  margin: 0 auto;
  border-top: none;
  border-left: none;
  border-right: none;
  box-sizing: border-box;
  width: 100%;
  overflow: hidden;
}

.cells_auto_fill.multi_cell {
  width: 748rpx;
  height: 528rpx;
  position: relative;
}

.cells_auto_fill.multi_cell .items {
  position: absolute;
  display: block;
  height: auto !important;
}

.cells_auto_fill .items {
  width: 100%;
  display: block;
  animation: opacity01;
  background-color: transparent;
  outline: 0;
  text-decoration: none;
}

.cells_auto_fill .items image {
  width: inherit;
  height: inherit;
}

.box-flex {
  display: flex;
}

.component-list-main .list_two_type1 .item:first-child {
  margin-right: 4rpx;
}

.component-list-main .list_two_type1 .item {
  width: 375rpx;
}

navigator {
  background-color: transparent;
  outline: 0;
  text-decoration: none;
}

.component-list-main .list_two_type1 .item .img {
  width: 375rpx;
  height: 375rpx;
  position: relative;
  overflow: hidden;
}

.component-list-main .info {
  padding: 20rpx 28rpx;
  text-align: left;
}

.component-list-main .brief, .component-list-main .name {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.component-list-main .name {
  font-size: 29rpx;
  color: rgba(0, 0, 0, 0.87);
  display: block;
}

.component-list-main .brief {
  margin-top: 6rpx;
  font-size: 25rpx;
  line-height: 31rpx;
  color: rgba(0, 0, 0, 0.54);
  display: block;
}

.component-list-main .price {
  display: inline-block;
  font-size: 29rpx;
  color: #ea625b;
  height: 1.5em;
  line-height: 1.5em;
  position: relative;
  padding-left: 0.5em;
}

.component-list-main .price::before {
  content: "\A5";
  position: absolute;
  left: 0;
  top: 0;
  font-size: 0.76em;
  text-decoration: none;
}

.component-list-main .price .old {
  display: inline-block;
  margin: 0 10rpx;
  font-size: 23rpx;
  color: rgba(0, 0, 0, 0.54);
}

.component-list-main .price span {
  display: inline-block;
  margin-left: 0.04rem;
  font-size: 0.2rem;
}
